    "dev:debug": "nodemon --inspect app.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gflujan/LoLProWinsApp.git"
