    "npm": ">=6.8.0"
  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "d3": "^5.9.2",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "node-fetch": "^2.3.0",
